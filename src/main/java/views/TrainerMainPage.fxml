<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.views.TrainerMainPageViewController">
  <children>
    <VBox layoutY="87.0" prefHeight="200.0" prefWidth="100.0" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button fx:id="backBt" mnemonicParsing="false" onAction="#onBackBtAction" text="←" />
        <HBox spacing="50.0">
          <children>
            <Text fx:id="welcomeTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome {name} from {city}">
              <font>
                <Font size="18.0" />
              </font>
            </Text>
          </children>
          <opaqueInsets>
            <Insets />
          </opaqueInsets>
        </HBox>
        <VBox prefHeight="209.0" prefWidth="575.0" spacing="25.0">
          <children>
            <Button fx:id="startShoppingBt" mnemonicParsing="false" onAction="#onStartShoppingBtAction" prefHeight="25.0" prefWidth="110.0" text="Start shopping" />
            <Button fx:id="shoppingHistoryBt" mnemonicParsing="false" onAction="#onShoppingHistoryBtAction" prefHeight="25.0" prefWidth="110.0" text="Shopping history" />
            <Button fx:id="cureHistoryBt1" layoutX="10.0" layoutY="60.0" mnemonicParsing="false" onAction="#onCureHistoryBtAction" prefHeight="25.0" prefWidth="108.0" text="Cure history" />
          </children>
        </VBox>
      </children>
      <padding>
        <Insets left="25.0" top="25.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
