<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.views.NurseMainPageViewController">
  <children>
    <VBox layoutY="87.0" prefHeight="200.0" prefWidth="100.0" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Button fx:id="backBt" mnemonicParsing="false" onAction="#onBackBtAction" text="←" />
        <HBox spacing="50.0">
          <children>
            <Hyperlink fx:id="editOwnRegisterHl" onAction="#onEditOwnRegisterHlAction" text="Edit account" underline="true" />
            <Text fx:id="welcomeTxt" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome {name} from {city}">
              <font>
                <Font size="18.0" />
              </font>
            </Text>
          </children>
          <opaqueInsets>
            <Insets />
          </opaqueInsets>
        </HBox>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="25.0">
          <children>
            <ComboBox fx:id="trainersCb" onAction="#onTrainersCbAction" prefHeight="25.0" prefWidth="170.0" promptText="Choose trainer" />
            <Button fx:id="registerCuresBt" mnemonicParsing="false" onAction="#onRegisterCuresBtAction" text="Register cures" />
            <Button fx:id="cureHistoryBt" mnemonicParsing="false" onAction="#onCureHistoryBtAction" text="Cure history" />
            <Hyperlink fx:id="managerTrainersHl" onAction="#onManagerTrainersHlAction" text="Manage trainers" underline="true" />
          </children>
        </VBox>
      </children>
      <padding>
        <Insets left="25.0" top="25.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
